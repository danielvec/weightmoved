<h2>Workout Log</h2>
<table>
    <thead>
        <tr>
            <th>Date</th>
            <th>Total</th>
        </tr>
    </thead>
    <tbody>
        <% @workouts.each do |workout| %>
            <tr>
            <td><%= workout.date %></td>
            <td><%= workout.total %></td>
            </tr>
        <% end %>
    </tbody>
</table>